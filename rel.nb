          RowBox[{
           RowBox[{"q2nr", "[", "0", "]"}], "==", "y2p"}], ",", 
          RowBox[{
           RowBox[{"q1nr", "[", "t", "]"}], "==", "y1"}], ",", 
          RowBox[{
           RowBox[{"q2nr", "[", "t", "]"}], "==", "y2"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"q1nr", "[", "ti", "]"}], ",", 
